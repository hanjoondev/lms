<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.hanjoon.lms.banner.mapper.BannerMapper">
    <select id="selectListCount" resultType="long">
        select count(*)
        from banner
    </select>
    <select id="selectList" resultType="dev.hanjoon.lms.banner.dto.BannerDto">
        select *
        from banner
        order by created_at desc
        limit #{pageStart}, #{pageEnd}
    </select>
</mapper>
